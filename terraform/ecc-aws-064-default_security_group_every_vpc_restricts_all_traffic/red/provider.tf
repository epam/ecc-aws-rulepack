terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 5"
    }
  }
}

provider "aws"{
  profile = var.profile
  region = var.default-region

  default_tags {
    tags = {
      CustodianRule    = "ecc-aws-064-default_security_group_every_vpc_restricts_all_traffic"
      ComplianceStatus = "Red"
    }
  }
}