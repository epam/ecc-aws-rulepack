terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 5"
    }
  }
}

provider "aws"{
  profile = var.profile
  region = var.default-region

  default_tags {
    tags = {
      CustodianRule    = "ecc-aws-068-s3_bucket_cloudtrail_logs_not_publicly_accessible"
      ComplianceStatus = "Green"
    }
  }
}