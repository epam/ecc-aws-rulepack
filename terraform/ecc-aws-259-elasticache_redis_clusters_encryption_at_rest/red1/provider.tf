terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 4"
    }
  }
}

provider "aws" {
  profile = var.profile
  region  = var.default-region

  default_tags {
    tags = {
      CustodianRule    = "ecc-aws-259-elasticache_redis_clusters_encryption_at_rest"
      ComplianceStatus = "Red1"
    }
  }
}