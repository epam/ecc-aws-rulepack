terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 4"
    }
  }
}

provider "aws" {
  profile = var.profile
  region  = var.default-region

  default_tags {
    tags = {
      CustodianRule    = "ecc-aws-958-cloudfront_s3_origin_non_existent_bucket"
      ComplianceStatus = "Green1"
    }
  }
}
